#include "main.h"

void custom_print(int n)
{
	int count = 0;
	int number = n;
	int i, k, j;
	
	do {
		n /= 10;
		++count;
	} while (n != 0);

	for (i = 1; i <= count; i++)
	{
		for (k = 1; k < n; k++)
		{
			loop *= 10;
		}
		
		/*Determining how much*/
		
		for (j = (n - 1); j >= 1; j--)
		{
			int temp_number;
			
			temp_number = number / loop;
			_putchar((temp_number % 10) + '0'); 
		}
	}
	
}
